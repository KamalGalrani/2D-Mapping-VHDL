vsim work.transmit
# vsim work.transmit 
# Loading std.standard
# Loading std.textio(body)
# Loading ieee.std_logic_1164(body)
# Loading work.transmit(main)
add wave -position insertpoint  \
sim:/transmit/clk
add wave -position insertpoint  \
sim:/transmit/clk
add wave -position insertpoint  \
sim:/transmit/wr
add wave -position insertpoint  \
sim:/transmit/data
add wave -position insertpoint  \
sim:/transmit/tx
add wave -position insertpoint  \
sim:/transmit/txrdy
add wave -position insertpoint  \
sim:/transmit/done
add wave -position insertpoint  \
sim:/transmit/trigger
add wave -position insertpoint  \
sim:/transmit/parity
add wave -position insertpoint  \
sim:/transmit/latch
add wave -position insertpoint  \
sim:/transmit/step
add wave -position insertpoint  \
sim:/transmit/clk
force -freeze sim:/transmit/clk 1 0, 0 {50000 ps} -r 100000
force -freeze sim:/transmit/wr 1 0, 0 {990000 ps} -r 1000000
force reset 1 0 ps, 0 200000 ps
force data 01000010 0
run -all
